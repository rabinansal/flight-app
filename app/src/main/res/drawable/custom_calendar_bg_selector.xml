<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true" android:drawable="@drawable/upper_bg"/>
    <item android:state_pressed="true" android:drawable="@drawable/upper_bg"/>
    <item app:tsquare_state_current_month="true"
        android:drawable="@drawable/bg_circle_today">
    </item>
    <item app:tsquare_state_current_month="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/md_white_1000"/>
        </shape>
    </item>


</selector>
